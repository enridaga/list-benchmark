prefix midi: <http://purl.org/midi-ld/midi#>
prefix midi-note: <http://purl.org/midi-ld/notes/>
prefix midi-prog: <http://purl.org/midi-ld/programs/>
prefix prov: <http://www.w3.org/ns/prov#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xml: <http://www.w3.org/XML/1998/namespace>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

WITH <http://purl.org/midi-ld/piece/8cf9897535d79e68c33a3076aa06d073>
DELETE {
	<http://purl.org/midi-ld/piece/8cf9897535d79e68c33a3076aa06d073/track00> midi:hasEvent ?e .
	?e midi:id 23789 .
	?event midi:id ?oldId
}
INSERT {
	?event midi:id ?newId	
}
WHERE {

{
SELECT ?event ?oldId ?newId WHERE {

	<http://purl.org/midi-ld/piece/8cf9897535d79e68c33a3076aa06d073/track00> a midi:Track ;
			midi:hasEvent ?event .
			?event midi:id ?oldId .
	FILTER ( ?oldId > 23789 ) .
	BIND ((?oldId-1) AS ?newId)
}
}
}
